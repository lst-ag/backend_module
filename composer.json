{
  "name": "lst/backend-module",
  "type": "typo3-cms-extension",
  "description": "A convenience extension for quickly creating TYPO3 backend modules",
  "keywords": [
    "typo3",
    "backend",
    "module"
  ],
  "authors": [
    {
      "name": "Christian Fries",
      "email": "christian.fries@lst.team",
      "role": "Developer"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "~9.5.17 || ~10.4.2"
  },
  "replace": {
    "chf/backend-module": "*"
  },
  "autoload": {
    "psr-4": {
      "LST\\BackendModule\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "backend_module"
    }
  }
}
